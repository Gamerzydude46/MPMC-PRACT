exp02_01
; You may customize this and other start-up templates; 
; The location of this template is c:\emu8086\inc\0_com_template.txt

org 100h

.model small
.data
.code
main proc

mov cx,10  ;setting counter 
mov dx,48  ;ASCI value of zero 

L1:        ;label
mov ah,2   ;display 
int 21h
add dx,1   ;increment (can also use INC instead of ADD)

loop L1    ;loop

mov ah,4ch
int 21h

main endp

ret

Result:
0123456789

exp02_03
; You may customize this and other start-up templates; 
; The location of this template is c:\emu8086\inc\0_com_template.txt

org 100h
         
.model small
.data
.code
main proc  
    
mov al,7     ;assiging value to al reg
mov bl,4     ;assigning value to bl reg
sub al,bl    ;subtraction logic
add al,48    ;adding zero   

mov dl,al   ;moving al to dl
mov ah,2    ;display
int 21h  

main endp
ret

Result:
3
